<procedure>
      <procedureName>DeployRubyGems</procedureName>
      <description>Invokes the BuildOps x_rb_deployRubyGems procedure. This procedure runs on all build agents. It checks to see whether ec-rbenv is able to be run on the agent, and if so, then it pulls down the Ruby gems specified by the invoking project's "Gems" property sheet.</description>
      <jobNameTemplate/>
      <resourceName/>
      <timeLimit/>
      <timeLimitUnits>minutes</timeLimitUnits>
      <workspaceName/>
      <acl>
        <inheriting>1</inheriting>
      </acl>
      <propertySheet>
        <acl>
          <inheriting>1</inheriting>
        </acl>
      </propertySheet>
      <step>
        <stepName>DeployRubyGems</stepName>
        <alwaysRun>0</alwaysRun>
        <broadcast>0</broadcast>
        <condition/>
        <description/>
        <errorHandling>failProcedure</errorHandling>
        <exclusiveMode>none</exclusiveMode>
        <parallel>0</parallel>
        <precondition/>
        <releaseMode>none</releaseMode>
        <resourceName/>
        <subprocedure>x_rb_deployRubyGems</subprocedure>
        <subproject>BuildOps</subproject>
        <timeLimit/>
        <timeLimitUnits>minutes</timeLimitUnits>
        <workspaceName/>
        <acl>
          <inheriting>1</inheriting>
        </acl>
        <actualParameters>
          <acl>
            <inheriting>1</inheriting>
          </acl>
          <property>
            <propertyName>deployPool</propertyName>
            <expandable>1</expandable>
            <value>AptBased</value>
          </property>
          <property>
            <propertyName>specificGemName</propertyName>
            <expandable>1</expandable>
            <value/>
          </property>
        </actualParameters>
        <propertySheet>
          <acl>
            <inheriting>1</inheriting>
          </acl>
        </propertySheet>
      </step>
    </procedure>
